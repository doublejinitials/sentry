<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <stroke android:color="#60ffffff" android:width="1dp"/>
        </shape>
    </item>
    <item
            android:top="1dp"
            android:bottom="1dp"
            android:left="1dp"
            android:right="1dp">
        <shape android:shape="oval">
            <stroke android:color="#60000000" android:width="2dp"/>
        </shape>
    </item>
    <item
            android:top="4dp"
            android:bottom="4dp"
            android:left="4dp"
            android:right="4dp">
        <shape android:shape="oval">
            <stroke android:color="#60ffffff" android:width="1dp"/>
        </shape>
    </item>
</layer-list>